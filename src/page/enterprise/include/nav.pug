#carousel-example-generic.carousel.slide.banner(data-ride="carousel")
  ol.carousel-indicators
    li.active(data-slide-to="0", data-target="#carousel-example-generic")
    li(data-slide-to="1", data-target="#carousel-example-generic")
    li(data-slide-to="2", data-target="#carousel-example-generic")
  .carousel-inner(role="listbox")
    .carousel-item.active
      img.d-block.w-100(src="https://wallpapers.com/images/high/jojo-bizarre-adventure-hero-banners-97au0jin9lw0i6lz.jpg", alt="")
    .carousel-item
      img.d-block.w-100(src="https://gamingtrend.com/wp-content/uploads/2018/08/jojosbizarreadventure-anime-keyart-s1tos3.jpg", alt="")
    .carousel-item
      img.d-block.w-100(src="https://www.yifengame.com/wp-content/uploads/2021/08/20210807031646-610dfb1e6c394.jpg", alt="")
  button.carousel-control-prev(type="button", data-target="#carousel-example-generic", data-slide="prev")
    span.carousel-control-prev-icon(aria-hidden="true")
    span.sr-only Previous
  button.carousel-control-next(type="button", data-target="#carousel-example-generic", data-slide="next")
    span.carousel-control-next-icon(aria-hidden="true")
    span.sr-only Next
